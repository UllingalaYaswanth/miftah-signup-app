{
  "expo": {
    "name": "miftah",
    "slug": "miftah",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/miftah_logo.png",
    "scheme": "miftah",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/miftah_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yaswanthullingala.miftah"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/miftah_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#f0f0f0",
          "enableFullScreenImage_legacy": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2e072d98-ae95-4206-8c0b-3c1ac75367cd"
      }
    }
  }
}
